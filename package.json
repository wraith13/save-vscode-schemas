{
    "name": "save-vscode-schemas",
    "displayName": "save-vscode-schemas",
    "description": "Save VS Code's schemas",
    "version": "0.0.1",
    "license": "SEE LICENSE IN LICENSE_1_0.txt",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:save-vscode-schemas"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "save-vscode-schemas",
                "title": "Save VS Code's Schemas"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.48.0",
        "@types/glob": "^7.1.3",
        "@types/node": "^14.0.27",
        "eslint": "^7.6.0",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "glob": "^7.1.6",
        "typescript": "^3.8.3"
    }
}
